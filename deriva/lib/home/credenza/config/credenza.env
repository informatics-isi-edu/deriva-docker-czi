CREDENZA_DEFAULT_REALM=default
CREDENZA_ENABLE_LEGACY_API=true
CREDENZA_ENABLE_REFRESH_WORKER=true
CREDENZA_AUDIT_USE_SYSLOG=false
CREDENZA_REFRESH_WORKER_POLL_INTERVAL=60
CREDENZA_REFRESH_WORKER_EXPIRY_THRESHOLD=300
CREDENZA_STORAGE_BACKEND=postgresql
CREDENZA_STORAGE_BACKEND_URL="postgresql://credenza:${POSTGRES_CREDENZA_PASSWORD}@${POSTGRES_HOST}"
CREDENZA_ENCRYPT_SESSION_DATA=false
CREDENZA_ENCRYPTION_KEY="${CREDENZA_ENCRYPTION_KEY}"
CREDENZA_BASE_URL="https://${HOSTNAME}/authn"
CREDENZA_POST_LOGOUT_REDIRECT_URL="https://${HOSTNAME}"
CREDENZA_AUDIT_LOGFILE_PATH=/home/credenza/credenza-audit.log